                      "example": "low"
                    },
                    "confidence_reason": {
                      "type": "string",
                      "example": "string"
                    },
                    "status": {
                      "type": "string",
                      "example": "PASSED"
                    }
                  }
                },
                "examples": {
                  "200": {
                    "value": {
                      "acceptance_level": "high",
                      "acceptance_reason": "string",
                      "claims": [
                        {
                          "property": "placement_in_gender",
                          "value": "548th of 1172"
                        }
                      ],
                      "confidence_level": "low",
                      "confidence_reason": "string",
                      "status": "PASSED"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "parameters": [
        {
          "name": "request_template_id",
          "in": "path",
          "required": true,
          "schema": {
            "type": "string",
            "example": "req_01JZ64C16A8G2JVF79B0P5XD3A"
          }
        }
      ]
    }
