{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Inklink Developer Documentation",
  "colors": {
    "primary": "#ff1100",
    "light": "#f5f5f5",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "pages": ["index", "authentication", "quickstart"]
          },
          {
            "group": "Requests",
            "pages": ["essentials/settings", "essentials/navigation"]
          },
          {
            "group": "Writing content",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/reusable-snippets"
            ]
          },
          {
            "group": "AI tools",
            "pages": [
              "ai-tools/cursor",
              "ai-tools/claude-code",
              "ai-tools/windsurf"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "groups": [
          {
            "group": "Endpoint examples",
            "pages": [
              "api-reference/endpoint/create",
              "api-reference/endpoint/createSession",
              "api-reference/endpoint/get"
            ]
          }
        ]
      }
    ]
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
